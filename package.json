{
  "name": "jizy-niceselect",
  "version": "1.0.0",
  "browser": "dist/js/jizy-niceselect.min.js",
  "main": "lib/index.js",
  "type": "module",
  "jizy": "dist/",
  "files": [
    "dist/*",
    "example/*",
    "lib/*"
  ],
  "scripts": {
    "jpack:example": "node ./cli/jpack.js --action build --name example",
    "jpack:example-debug": "node ./cli/jpack.js --action build --name example --debug",
    "jpack:export": "node ./cli/jpack.js --action build --name perso",
    "jpack:export-debug": "node ./cli/jpack.js --action build --name perso --debug",
    "jpack:dist": "node ./cli/jpack.js",
    "jpack:dist-debug": "node ./cli/jpack.js --debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joffreydemetz/jizy-niceselect.git"
  },
  "keywords": [
    "jdz",
    "jizy"
  ],
  "author": "Joffrey Demetz <joffrey.demetz@gmail.com> (https://joffreydemetz.com/)",
  "license": "MIT",
  "description": "A lightweight, customizable and accessible select box replacement library.",
  "devDependencies": {
    "jizy-packer": "^2.1.32",
    "less": "^4.4.1"
  }
}